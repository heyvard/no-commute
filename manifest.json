{
  "name": "Strava commute hider",
  "version": "0.1",
  "description": "Removes commutes from strava. Please include this feature in strava",
  "icons": {
    "128": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.strava.com/*"
      ],
      "js": [
        "libs/jquery-3.2.1.min.js",
        "no-commute.js"
      ]
    }
  ],
  "permissions": [
    "tabs"
  ],
  "manifest_version": 2
}
